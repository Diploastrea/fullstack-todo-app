{
  "name": "backend",
  "version": "1.0.0",
  "jest": {
    "globalSetup": "./scripts/jestGlobalSetup.js",
    "globalTeardown": "./scripts/jestGlobalTeardown.js"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix",
    "test": "jest --watchAll --verbose --collectCoverage --detectOpenHandles",
    "test:ci": "jest --ci --collectCoverage --detectOpenHandles --reporters=default",
    "test:cover": "jest --collectCoverage",
    "start": "nodemon --exec ./node_modules/.bin/babel-node src/index.js",
    "build": "babel src -d dist",
    "serve": "node -r dotenv/config dist/index.js",
    "sequelize": "sequelize init",
    "migration": "npx sequelize-cli db:migrate",
    "migration:data": "npx sequelize-cli db:seed:all",
    "migration:all": "npm run migration && npm run migration:data",
    "migration:undo": "npx sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assert": "^2.0.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "sequelize": "^6.21.2",
    "umzug": "^3.2.1",
    "validator": "^13.7.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/node": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.3",
    "babel-jest": "^28.1.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-restricted-globals": "^0.2.0",
    "jest": "^28.1.1",
    "nodemon": "^1.3.3",
    "prettier": "2.7.1",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3"
  }
}
